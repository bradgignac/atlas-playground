{
  "name": "atlas-playground-client",
  "version": "0.0.0",
  "description": "Client for Atlas Playground demo application.",
  "homepage": "https://github.com/bradgignac/atlas-playground",
  "author": "Brad Gignac <bgignac@bradgignac.com>",
  "license": "MIT",
  "main": "lib/client.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run build-js && npm run build-less && npm run build-html && npm run build-fonts",
    "build-js": "browserify -t [ reactify --es6 ] lib/js/client.jsx > dist/atlas-playground-client.js",
    "build-less": "lessc lib/less/client.less --include-path=node_modules > dist/atlas-playground-client.css",
    "build-html": "cp lib/html/index.html dist/index.html",
    "build-fonts": "cp -r node_modules/flat-ui/fonts dist",
    "watch": "npm run watch-js & npm run watch-less & npm run watch-html & npm run watch-fonts & http-server dist & livereload dist",
    "watch-js": "watch 'npm run build-js' lib/js",
    "watch-less": "watch 'npm run build-less' lib/less",
    "watch-html": "watch 'npm run build-html' lib/html",
    "watch-fonts": "watch 'npm run build-fonts' node_modules/flat-ui/fonts",
    "lint": "jshint lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bradgignac/atlas-playground"
  },
  "bugs": {
    "url": "https://github.com/bradgignac/atlas-playground/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.1",
    "flat-ui": "^2.1.1",
    "jquery": "^2.1.3",
    "react": "^0.12.2",
    "react-bootstrap": "^0.13.0"
  },
  "devDependencies": {
    "browserify": "^8.0.3",
    "http-server": "^0.7.4",
    "jshint": "^2.5.11",
    "less": "^2.2.0",
    "livereload": "^0.3.4",
    "reactify": "^0.17.1",
    "watch": "^0.13.0"
  },
  "private": true
}
